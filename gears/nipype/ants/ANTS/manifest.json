{
  "name": "nipype-interfaces-ants-registration-ants", 
  "description": "", 
  "author": "Yaroslav O. Halchenko", 
  "maintainer": "Yaroslav O. Halchenko <debian@onerussian.com>", 
  "license": "BSD-3-Clause", 
  "version": "0.0.2.nipype.1.0.2.1", 
  "config": {
    "affine_gradient_descent_option": {
      "type": "array", 
      "description": "Affine gradient descent option", 
      "optional": true, 
      "items": {
        "type": "number"
      }
    }, 
    "args": {
      "type": "string", 
      "description": "Additional parameters to the command", 
      "optional": true
    }, 
    "delta_time": {
      "type": "number", 
      "description": "Delta time", 
      "optional": true
    }, 
    "dimension": {
      "type": "integer", 
      "description": "image dimension (2 or 3) [default=3]", 
      "default": 3, 
      "optional": true, 
      "enum": [
        3, 
        2
      ]
    }, 
    "gradient_step_length": {
      "type": "number", 
      "description": "Gradient step length", 
      "optional": true
    }, 
    "metric": {
      "type": "array", 
      "description": "Metric [default=['CC']]", 
      "default": [
        "CC"
      ], 
      "items": {
        "type": "string"
      }
    }, 
    "metric_weight": {
      "type": "array", 
      "description": "the metric weight(s) for each stage. The weights must sum to 1 per stage. [default=[1.0]]", 
      "default": [
        1.0
      ], 
      "items": {
        "type": "number"
      }
    }, 
    "mi_option": {
      "type": "array", 
      "description": "Mi option", 
      "optional": true, 
      "items": {
        "type": "integer"
      }
    }, 
    "num_threads": {
      "type": "integer", 
      "description": "Number of ITK threads to use [default=1]", 
      "default": 1, 
      "optional": true
    }, 
    "number_of_affine_iterations": {
      "type": "array", 
      "description": "Number of affine iterations [default=[10000, 10000, 10000, 10000, 10000]]", 
      "default": [
        10000, 
        10000, 
        10000, 
        10000, 
        10000
      ], 
      "optional": true, 
      "items": {
        "type": "integer"
      }
    }, 
    "number_of_iterations": {
      "type": "array", 
      "description": "Number of iterations [default=[50, 35, 15]]", 
      "default": [
        50, 
        35, 
        15
      ], 
      "optional": true, 
      "items": {
        "type": "integer"
      }
    }, 
    "number_of_time_steps": {
      "type": "number", 
      "description": "Number of time steps", 
      "optional": true
    }, 
    "output_transform_prefix": {
      "type": "string", 
      "description": "Output transform prefix [default=out]", 
      "default": "out"
    }, 
    "radius": {
      "type": "array", 
      "description": "radius of the region (i.e. number of layers around a voxel point) that is used for computing cross correlation [default=[1]]", 
      "default": [
        1
      ], 
      "items": {
        "type": "integer"
      }
    }, 
    "regularization": {
      "type": "string", 
      "description": "Regularization [default=Gauss]", 
      "default": "Gauss", 
      "optional": true, 
      "enum": [
        "Gauss", 
        "DMFFD"
      ]
    }, 
    "regularization_deformation_field_sigma": {
      "type": "number", 
      "description": "Regularization deformation field sigma [default=0]", 
      "default": 0, 
      "optional": true
    }, 
    "regularization_gradient_field_sigma": {
      "type": "number", 
      "description": "Regularization gradient field sigma [default=3]", 
      "default": 3, 
      "optional": true
    }, 
    "smoothing_sigmas": {
      "type": "array", 
      "description": "Smoothing sigmas", 
      "optional": true, 
      "items": {
        "type": "integer"
      }
    }, 
    "subsampling_factors": {
      "type": "array", 
      "description": "Subsampling factors", 
      "optional": true, 
      "items": {
        "type": "integer"
      }
    }, 
    "symmetry_type": {
      "type": "number", 
      "description": "Symmetry type", 
      "optional": true
    }, 
    "transformation_model": {
      "type": "string", 
      "description": "Transformation model [default=SyN]", 
      "default": "SyN", 
      "enum": [
        "Diff", 
        "Elast", 
        "Exp", 
        "Greedy Exp", 
        "SyN"
      ]
    }, 
    "use_histogram_matching": {
      "type": "boolean", 
      "description": "Use histogram matching", 
      "optional": true
    }
  }, 
  "inputs": {
    "fixed_image": {
      "description": "image to which the moving image is warped", 
      "base": "file"
    }, 
    "moving_image": {
      "description": "image to apply transformation to (generally a coregisteredfunctional)", 
      "base": "file"
    }
  }, 
  "url": "http://nipype.readthedocs.io/en/1.0.2/interfaces/generated/interfaces.ants/registration.html", 
  "source": "https://github.com/yarikoptic/gearificator", 
  "custom": {
    "gearificator": {
      "interface": "nipype.interfaces.ants.registration:ANTS", 
      "outputs": {
        "affine_transform": {
          "description": "Affine transform file"
        }, 
        "inverse_warp_transform": {
          "description": "Inverse warping deformation field"
        }, 
        "metaheader": {
          "description": "VTK metaheader .mhd file"
        }, 
        "metaheader_raw": {
          "description": "VTK metaheader .raw file"
        }, 
        "warp_transform": {
          "description": "Warping deformation field"
        }
      }, 
      "category": "analysis"
    }, 
    "gear-builder": {
      "image": "nipype-interfaces-ants-registration-ants:0.0.2.nipype.1.0.2.1"
    }, 
    "docker-image": "nipype-interfaces-ants-registration-ants:0.0.2.nipype.1.0.2.1"
  }, 
  "label": "ANTS"
}