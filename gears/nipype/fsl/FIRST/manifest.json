{
  "name": "nipype-interfaces-fsl-preprocess-first", 
  "description": "", 
  "author": "Yaroslav O. Halchenko", 
  "maintainer": "Yaroslav O. Halchenko <debian@onerussian.com>", 
  "license": "Other", 
  "version": "0.0.2.nipype.1.0.2.1", 
  "config": {
    "args": {
      "type": "string", 
      "description": "Additional parameters to the command", 
      "optional": true
    }, 
    "brain_extracted": {
      "type": "boolean", 
      "description": "Input structural image is already brain-extracted", 
      "optional": true
    }, 
    "list_of_specific_structures": {
      "type": "array", 
      "description": "Runs only on the specified structures (e.g. L_Hipp, R_HippL_Accu, R_Accu, L_Amyg, R_AmygL_Caud, R_Caud, L_Pall, R_PallL_Puta, R_Puta, L_Thal, R_Thal, BrStem", 
      "optional": true, 
      "items": {
        "type": "string"
      }
    }, 
    "method": {
      "type": "string", 
      "description": "Method must be one of auto, fast, none, or it can be entered using the 'method_as_numerical_threshold' input [default=auto]", 
      "default": "auto", 
      "optional": true, 
      "enum": [
        "auto", 
        "fast", 
        "none"
      ]
    }, 
    "method_as_numerical_threshold": {
      "type": "number", 
      "description": "Specify a numerical threshold value or use the 'method' input to choose auto, fast, or none", 
      "optional": true
    }, 
    "no_cleanup": {
      "type": "boolean", 
      "description": "Input structural image is already brain-extracted", 
      "optional": true
    }, 
    "output_type": {
      "type": "string", 
      "description": "FSL output type [default=NIFTI_GZ]", 
      "default": "NIFTI_GZ", 
      "optional": true, 
      "enum": [
        "NIFTI_PAIR", 
        "NIFTI_PAIR_GZ", 
        "NIFTI_GZ", 
        "NIFTI"
      ]
    }, 
    "verbose": {
      "type": "boolean", 
      "description": "Use verbose logging.", 
      "optional": true
    }
  }, 
  "inputs": {
    "affine_file": {
      "description": "Affine matrix to use (e.g. img2std.mat) (does not re-run registration)", 
      "optional": true, 
      "base": "file"
    }, 
    "in_file": {
      "description": "input data file", 
      "base": "file"
    }, 
    "out_file": {
      "description": "output data file [default=segmented]", 
      "default": "segmented", 
      "base": "file"
    }
  }, 
  "url": "http://nipype.readthedocs.io/en/1.0.2/interfaces/generated/interfaces.ants/registration.html", 
  "source": "https://github.com/yarikoptic/gearificator", 
  "custom": {
    "gearificator": {
      "interface": "nipype.interfaces.fsl.preprocess:FIRST", 
      "outputs": {
        "bvars": {
          "description": "bvars for each subcortical region"
        }, 
        "original_segmentations": {
          "description": "3D image file containing the segmented regions as integer values. Uses CMA labelling"
        }, 
        "segmentation_file": {
          "description": "4D image file containing a single volume per segmented region"
        }, 
        "vtk_surfaces": {
          "description": "VTK format meshes for each subcortical region"
        }
      }, 
      "category": "analysis"
    }, 
    "gear-builder": {
      "image": "nipype-interfaces-fsl-preprocess-first:0.0.2.nipype.1.0.2.1"
    }, 
    "docker-image": "nipype-interfaces-fsl-preprocess-first:0.0.2.nipype.1.0.2.1"
  }, 
  "label": "FIRST"
}