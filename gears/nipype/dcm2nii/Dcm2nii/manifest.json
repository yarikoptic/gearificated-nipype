{
  "name": "nipype-interfaces-dcm2nii-dcm2nii", 
  "description": "Uses MRIcron's dcm2nii to convert dicom files", 
  "author": "Yaroslav O. Halchenko", 
  "maintainer": "Yaroslav O. Halchenko <debian@onerussian.com>", 
  "license": "BSD-3-Clause", 
  "version": "0.0.2.nipype.1.0.2.1", 
  "config": {
    "anonymize": {
      "type": "boolean", 
      "description": "Remove identifying information [default=True]", 
      "default": true, 
      "optional": true
    }, 
    "args": {
      "type": "string", 
      "description": "Additional parameters to the command", 
      "optional": true
    }, 
    "collapse_folders": {
      "type": "boolean", 
      "description": "Collapse input folders [default=True]", 
      "default": true, 
      "optional": true
    }, 
    "convert_all_pars": {
      "type": "boolean", 
      "description": "Convert every image in directory [default=True]", 
      "default": true, 
      "optional": true
    }, 
    "date_in_filename": {
      "type": "boolean", 
      "description": "Date in filename [default=True]", 
      "default": true, 
      "optional": true
    }, 
    "events_in_filename": {
      "type": "boolean", 
      "description": "Events (series/acq) in filename [default=True]", 
      "default": true, 
      "optional": true
    }, 
    "gzip_output": {
      "type": "boolean", 
      "description": "Gzip output (.gz)", 
      "optional": true
    }, 
    "id_in_filename": {
      "type": "boolean", 
      "description": "ID  in filename", 
      "optional": true
    }, 
    "nii_output": {
      "type": "boolean", 
      "description": "Save as .nii - if no, create .hdr/.img pair [default=True]", 
      "default": true, 
      "optional": true
    }, 
    "output_dir": {
      "description": "Output dir - if unspecified, source directory is used [default=outputs/]", 
      "default": "outputs/", 
      "optional": true, 
      "base": "file"
    }, 
    "protocol_in_filename": {
      "type": "boolean", 
      "description": "Protocol in filename [default=True]", 
      "default": true, 
      "optional": true
    }, 
    "reorient": {
      "type": "boolean", 
      "description": "Reorient image to nearest orthogonal", 
      "optional": true
    }, 
    "reorient_and_crop": {
      "type": "boolean", 
      "description": "Reorient and crop 3D images", 
      "optional": true
    }, 
    "source_dir": {
      "description": "Source dir", 
      "base": "file", 
      "optional": true
    }, 
    "source_in_filename": {
      "type": "boolean", 
      "description": "Source filename", 
      "optional": true
    }, 
    "spm_analyze": {
      "type": "boolean", 
      "description": "SPM2/Analyze not SPM5/NIfTI", 
      "optional": true
    }
  }, 
  "inputs": {
    "config_file": {
      "description": "Load settings from specified inifile", 
      "optional": true, 
      "base": "file"
    }, 
    "source_names": {
      "description": "Source names", 
      "base": "file", 
      "optional": true
    }
  }, 
  "url": "http://nipype.readthedocs.io/en/1.0.2/interfaces/generated/interfaces.ants/registration.html", 
  "source": "https://github.com/yarikoptic/gearificator", 
  "custom": {
    "gearificator": {
      "interface": "nipype.interfaces.dcm2nii:Dcm2nii", 
      "outputs": {
        "bvals": {
          "description": "Bvals"
        }, 
        "bvecs": {
          "description": "Bvecs"
        }, 
        "converted_files": {
          "description": "Converted files"
        }, 
        "reoriented_and_cropped_files": {
          "description": "Reoriented and cropped files"
        }, 
        "reoriented_files": {
          "description": "Reoriented files"
        }
      }, 
      "category": "analysis"
    }, 
    "gear-builder": {
      "image": "nipype-interfaces-dcm2nii-dcm2nii:0.0.2.nipype.1.0.2.1"
    }, 
    "docker-image": "nipype-interfaces-dcm2nii-dcm2nii:0.0.2.nipype.1.0.2.1"
  }, 
  "label": "Dcm2nii"
}