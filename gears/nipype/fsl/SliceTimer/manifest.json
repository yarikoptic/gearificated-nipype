{
  "name": "nipype-interfaces-fsl-preprocess-slicetimer", 
  "description": " use FSL slicetimer to perform slice timing correction.", 
  "author": "Yaroslav O. Halchenko", 
  "maintainer": "Yaroslav O. Halchenko <debian@onerussian.com>", 
  "license": "Other", 
  "version": "0.0.2.nipype.1.0.2.1", 
  "config": {
    "args": {
      "type": "string", 
      "description": "Additional parameters to the command", 
      "optional": true
    }, 
    "global_shift": {
      "type": "number", 
      "description": "shift in fraction of TR, range 0:1 (default is 0.5 = no shift)", 
      "optional": true
    }, 
    "index_dir": {
      "type": "boolean", 
      "description": "slice indexing from top to bottom", 
      "optional": true
    }, 
    "interleaved": {
      "type": "boolean", 
      "description": "use interleaved acquisition", 
      "optional": true
    }, 
    "output_type": {
      "type": "string", 
      "description": "FSL output type [default=NIFTI_GZ]", 
      "default": "NIFTI_GZ", 
      "optional": true, 
      "enum": [
        "NIFTI_PAIR", 
        "NIFTI_PAIR_GZ", 
        "NIFTI_GZ", 
        "NIFTI"
      ]
    }, 
    "slice_direction": {
      "type": "integer", 
      "description": "direction of slice acquisition (x=1, y=2, z=3) - default is z [default=1]", 
      "default": 1, 
      "optional": true, 
      "enum": [
        1, 
        2, 
        3
      ]
    }, 
    "time_repetition": {
      "type": "number", 
      "description": "Specify TR of data - default is 3s", 
      "optional": true
    }
  }, 
  "inputs": {
    "custom_order": {
      "description": "filename of single-column custom interleave order file (first slice is referred to as 1 not 0)", 
      "optional": true, 
      "base": "file"
    }, 
    "custom_timings": {
      "description": "slice timings, in fractions of TR, range 0:1 (default is 0.5 = no shift)", 
      "optional": true, 
      "base": "file"
    }, 
    "in_file": {
      "description": "filename of input timeseries", 
      "base": "file"
    }, 
    "out_file": {
      "description": "filename of output timeseries", 
      "optional": true, 
      "base": "file"
    }
  }, 
  "url": "http://nipype.readthedocs.io/en/1.0.2/interfaces/generated/interfaces.ants/registration.html", 
  "source": "https://github.com/yarikoptic/gearificator", 
  "custom": {
    "gearificator": {
      "interface": "nipype.interfaces.fsl.preprocess:SliceTimer", 
      "outputs": {
        "slice_time_corrected_file": {
          "description": "slice time corrected file"
        }
      }, 
      "category": "analysis"
    }, 
    "gear-builder": {
      "image": "nipype-interfaces-fsl-preprocess-slicetimer:0.0.2.nipype.1.0.2.1"
    }, 
    "docker-image": "nipype-interfaces-fsl-preprocess-slicetimer:0.0.2.nipype.1.0.2.1"
  }, 
  "label": "SliceTimer"
}