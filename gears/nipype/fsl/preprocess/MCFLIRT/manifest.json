{
  "name": "nipype-interfaces-fsl-preprocess-mcflirt", 
  "description": "Use FSL MCFLIRT to do within-modality motion correction.", 
  "author": "Yaroslav O. Halchenko", 
  "maintainer": "Yaroslav O. Halchenko <debian@onerussian.com>", 
  "license": "Other", 
  "version": "0.0.2.nipype.1.0.2.1", 
  "config": {
    "args": {
      "type": "string", 
      "description": "Additional parameters to the command", 
      "optional": true
    }, 
    "bins": {
      "type": "integer", 
      "description": "number of histogram bins", 
      "optional": true
    }, 
    "cost": {
      "type": "string", 
      "description": "cost function to optimize [default=mutualinfo]", 
      "default": "mutualinfo", 
      "enum": [
        "mutualinfo", 
        "woods", 
        "corratio", 
        "normcorr", 
        "normmi", 
        "leastsquares"
      ]
    }, 
    "dof": {
      "type": "integer", 
      "description": "degrees of freedom for the transformation", 
      "optional": true
    }, 
    "interpolation": {
      "type": "string", 
      "description": "interpolation method for transformation [default=spline]", 
      "default": "spline", 
      "enum": [
        "spline", 
        "nn", 
        "sinc"
      ]
    }, 
    "mean_vol": {
      "type": "boolean", 
      "description": "register to mean volume", 
      "optional": true
    }, 
    "output_type": {
      "type": "string", 
      "description": "FSL output type [default=NIFTI_GZ]", 
      "default": "NIFTI_GZ", 
      "enum": [
        "NIFTI_PAIR", 
        "NIFTI_PAIR_GZ", 
        "NIFTI_GZ", 
        "NIFTI"
      ]
    }, 
    "ref_vol": {
      "type": "integer", 
      "description": "volume to align frames to", 
      "optional": true
    }, 
    "rotation": {
      "type": "integer", 
      "description": "scaling factor for rotation tolerances", 
      "optional": true
    }, 
    "save_mats": {
      "type": "boolean", 
      "description": "save transformation matrices", 
      "optional": true
    }, 
    "save_plots": {
      "type": "boolean", 
      "description": "save transformation parameters", 
      "optional": true
    }, 
    "save_rms": {
      "type": "boolean", 
      "description": "save rms displacement parameters", 
      "optional": true
    }, 
    "scaling": {
      "type": "number", 
      "description": "scaling factor to use", 
      "optional": true
    }, 
    "smooth": {
      "type": "number", 
      "description": "smoothing factor for the cost function", 
      "optional": true
    }, 
    "stages": {
      "type": "integer", 
      "description": "stages (if 4, perform final search with sinc interpolation", 
      "optional": true
    }, 
    "stats_imgs": {
      "type": "boolean", 
      "description": "produce variance and std. dev. images", 
      "optional": true
    }, 
    "use_contour": {
      "type": "boolean", 
      "description": "run search on contour images", 
      "optional": true
    }, 
    "use_gradient": {
      "type": "boolean", 
      "description": "run search on gradient images", 
      "optional": true
    }
  }, 
  "inputs": {
    "in_file": {
      "description": "timeseries to motion-correct", 
      "base": "file"
    }, 
    "init": {
      "description": "inital transformation matrix", 
      "optional": true, 
      "base": "file"
    }, 
    "out_file": {
      "description": "file to write", 
      "optional": true, 
      "base": "file"
    }, 
    "ref_file": {
      "description": "target image for motion correction", 
      "optional": true, 
      "base": "file"
    }
  }, 
  "url": "http://nipype.readthedocs.io/en/1.0.2/interfaces/generated/interfaces.ants/registration.html", 
  "source": "https://github.com/yarikoptic/gearificator", 
  "custom": {
    "gearificator": {
      "interface": "nipype.interfaces.fsl.preprocess:MCFLIRT", 
      "outputs": {
        "mat_file": {
          "description": "transformation matrices"
        }, 
        "mean_img": {
          "description": "mean timeseries image (if mean_vol=True)"
        }, 
        "out_file": {
          "description": "motion-corrected timeseries"
        }, 
        "par_file": {
          "description": "text-file with motion parameters"
        }, 
        "rms_files": {
          "description": "absolute and relative displacement parameters"
        }, 
        "std_img": {
          "description": "standard deviation image"
        }, 
        "variance_img": {
          "description": "variance image"
        }
      }, 
      "category": "analysis"
    }, 
    "gear-builder": {
      "image": "nipype-interfaces-fsl-preprocess-mcflirt:0.0.2.nipype.1.0.2.1"
    }, 
    "docker-image": "nipype-interfaces-fsl-preprocess-mcflirt:0.0.2.nipype.1.0.2.1"
  }, 
  "label": "MCFLIRT"
}